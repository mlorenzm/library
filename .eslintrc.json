{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:node/recommended"
    ],
    "overrides": [
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "error",
        "no-console": "off",
    }
}
